{
  "builds": [
      {
          "src": "VolantProject/wsgi.py",
          "use": "@vercel/python",
          "config": {
              "maxLambdaSize": "15mb",
              "runtime": "python3.12.1"
          }
      },
      {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": {
              "distDir": "staticfiles_build"
          }
      },
      {
          "src": "manage.py",
          "use": "@vercel/python"
      },
      {
          "src": "staticfiles_build/**/*",
          "use": "@vercel/static"
      }
  ],
  "routes": [
      {
          "src": "/static/(.*)",
          "dest": "staticfiles_build/$1"
      },
      {
          "src": "/(.*)",
          "dest": "VolantProject/wsgi.py"
      },
      {
          "src": "/(.*)",
          "dest": "manage.py"
      }
  ]
}
